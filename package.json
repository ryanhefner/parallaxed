{
  "name": "parallaxed",
  "description": "Easily apply parallax effects to page elements as you scroll.",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "repository": "ryanhefner/parallaxed",
  "bugs": {
    "url": "https://github.com/ryanhefner/parallaxed/issues"
  },
  "homepage": "https://github.com/ryanhefner/parallaxed#readme",
  "keywords": [
    "parallax",
    "parallax-scrolling",
    "parallax-effects",
    "scrolling",
    "animation"
  ],
  "files": [
    "es",
    "index.js",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -rf es && rm -rf umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__,*.test.js --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.1",
    "greenkeeper-lockfile": "^1.15.1",
    "gzip-size": "^4.1.0",
    "jest": "^23.0.0",
    "pretty-bytes": "^5.0.0",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1"
  }
}
